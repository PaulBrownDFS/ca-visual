<div class="carouselContainer">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        {{#times 5}}
        <li data-target="#myCarousel" data-slide-to="{{@index}}" {{#if @first}} class="active" {{/if}}></li>
        {{/times}}
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">
        {{#times 5}}
          <div class="item {{#if @first }}active{{/if}} slideIndex{{inc @index}}">
            <a href="{{productURL}}">
              <picture>
                <!-- WebP -->
                <source type="image/webp" media="(max-width: 1023px)"  srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=600,462]&layer1=[src=/i/dfs/{{productImage.name}}{{#if mobileproductimage}}_mobile{{/if}}&{{#if mobileMImageCrop}}{{mobileMImageCrop}}&{{/if}}{{#if mobilepimagefullheight}}anchor=tl&w=600{{else if mobileSidebarImage.name}}w=600&h=384&anchor=tl&top=78{{/if}}]{{#if mobileSidebarImage.name}}&layer2=[/i/dfs/{{mobileSidebarImage.name}}&w=600]{{/if}}{{#if sideBarCharOnMobile }}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{mobileCharacterCrop}}&{{mobileCharacterSize}}&bottom={{sideBarCBottomMob}}&left={{sideBarCLeftMob}}&anchor=bl]{{/if}}&layer4=[src=/i/dfs/{{pricingImage.name}}{{#if pricingImageCrop}}&{{pricingImageCrop}}{{/if}}&anchor=br&right=0&bottom=30]{{#if EspotImage.name}}{{#if espot1mobile}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_mobileright}}&top={{EspotImage_mobiletop}}]{{/if}}{{/if}}&qlt={{#if mobileslidequality}}{{mobileslidequality}}{{else}}60{{/if}}&fmt.jpeg.chroma=1,1,1&fmt.jpeg.interlaced=true&fmt=webp">

                <source type="image/webp" media="(max-width: 1365px)" srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=1024,488]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&fmt.jpeg.chroma=1,1,1&w=1024&qlt={{#if desktopslidequality}}{{desktopslidequality}}{{else}}60{{/if}}&fmt.jpeg.interlaced=true&fmt=webp">

                <source type="image/webp" media="(min-width: 1366px)" srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=1366,619]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&h=619&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&qlt={{#if desktopslidequality}}{{desktopslidequality}}{{else}}60{{/if}}&fmt.jpeg.chroma=1,1,1&w=1366&fmt.jpeg.interlaced=true&fmt=webp">

                <!-- Jpg -->
                <source media="(max-width: 1023px)"  srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=600,462]&layer1=[src=/i/dfs/{{productImage.name}}{{#if mobileproductimage}}_mobile{{/if}}&{{#if mobileMImageCrop}}{{mobileMImageCrop}}&{{/if}}{{#if mobilepimagefullheight}}anchor=tl&w=600{{else if mobileSidebarImage.name}}w=600&h=384&anchor=tl&top=78{{/if}}]{{#if mobileSidebarImage.name}}&layer2=[/i/dfs/{{mobileSidebarImage.name}}&w=600]{{/if}}{{#if sideBarCharOnMobile }}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{mobileCharacterCrop}}&{{mobileCharacterSize}}&bottom={{sideBarCBottomMob}}&left={{sideBarCLeftMob}}&anchor=bl]{{/if}}&layer4=[src=/i/dfs/{{pricingImage.name}}{{#if pricingImageCrop}}&{{pricingImageCrop}}{{/if}}&anchor=br&right=0&bottom=30]{{#if EspotImage.name}}{{#if espot1mobile}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_mobileright}}&top={{EspotImage_mobiletop}}]{{/if}}{{/if}}&qlt={{#if mobileslidequality}}{{mobileslidequality}}{{else}}60{{/if}}">

                <source media="(max-width: 1365px)" srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=1024,488]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&fmt.jpeg.chroma=1,1,1&w=1024&qlt={{#if desktopslidequality}}{{desktopslidequality}}{{else}}60{{/if}}">

                <source media="(min-width: 1366px)" srcset="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=1366,619]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&h=619&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&qlt={{#if desktopslidequality}}{{desktopslidequality}}{{else}}60{{/if}}">

                <img src="https://5n60l42235zn1u0bktjuna29f.staging.bigcontent.io/i/dfs/?layer0=[canvas=1366,619]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&h=619&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&qlt={{#if desktopslidequality}}{{desktopslidequality}}{{else}}60{{/if}}">
              </picture>

              <div class="bannerTextContainer {{#if hasCountdown}} carCountdown {{/if}}" data-messageid=slide_{{inc @index}}>
                <!-- START:: V2 CountDown (PB) July 2nd 2018-->
                  <div class="countdownContainer countdown_v2 {{countdownClassName}}{{#if hasCountdown}} {{else}} nonCDText{{/if}}"
                        data-deadline="{{countdownDeadline}}"
                        data-startdays="{{daysBeforeEnd}}"
                        data-daterange1="{{textVariance1}}"
                        data-daterange2="{{textVariance2}}"
                        data-daterange3="{{textVariance3}}"
                        data-daterange4="{{textVariance4}}"
                        data-hidefinalmessage="{{#if hasCountdown}}false{{else}}true{{/if}}"
                        data-testdate="">
                    <h6 class="cdHeader"><!-- DEFAULT HEADER TEXT--></h6>
                      <section class="timer {{#if hasCountdown}} showCountdown{{else}}hideCountdown{{/if}}">
                        <img src="//dfs.a.bigcontent.io/v1/static/loading_please_wait?v30" alt="loading..">
                      </section>
                    <h6 class="cdFooter"><!-- DEFAULT FOOTER TEXT--></h6>
                  </div>
                  <div style="display:none;" class="cdEndMessage {{countdownClassName}}">
                    <h3>ENDS TODAY<br>Order direct until midnight</h3>
                  </div>
                <!-- END:: V2 CountDown (PB) July 2nd 2018-->
              </div>
            </a>
            {{#if EspotImage2.name }}
            <div class="linkableRoundel2">
              {{#if espot2URL }}
                <a href="{{espot2URL}}">
              {{/if}}
              <picture>
                <!-- webp -->

                  <!-- 600 Mobile roundel 2-->
                  <source type="image/webp" media="(max-width: 1023px)"  srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?w=80%&qlt=100&fmt.jpeg.chroma=1,1,1&fmt.jpeg.interlaced=true&fmt=webp" />

                  <!-- 1024 width roundel 2-->
                  <source type="image/webp" media="(max-width: 1365px)" srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?w=80%&qlt=100&fmt.jpeg.chroma=1,1,1&fmt.jpeg.interlaced=true&fmt=webp" />

                  <!-- 1366 width roundel 2-->
                    <source type="image/webp" media="(min-width: 1366px)" srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?qlt=90&fmt.jpeg.chroma=1,1,1&fmt.jpeg.interlaced=true&fmt=webp" />

                <!-- Jpg -->

                  <!-- 600 Mobile roundel 2-->
                    <source type="image/webp" media="(max-width: 1023px)"  srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?w=80%&qlt=100" />

                  <!-- 1024 width roundel 2-->

                    <source media="(max-width: 1365px)" srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?w=80%&qlt=100" />

                  <!-- 1366 width roundel 2-->
                    <source media="(min-width: 1366px)" srcset="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?qlt=90" />

                  <img src="//images.dfs.co.uk/i/dfs/{{EspotImage2.name}}?w=80%&qlt=100" alt ="{{EspotImage2.name}}"/>
              </picture>
              {{#if espot2URL }}
                </a>
              {{/if}}
            </div>
            {{/if}}
          </div>
          {{/times}}
      </div>

      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    {{#if productData}}
      <div class="productDetails">
        <h4>Live Product Data: >></h4>
        <h4>SKU: <span>{{productData.sku}}</span></h4>
        <h4>Range Name: <span>{{productData.reviews.text}}</span></h4>
        <h4>Name: <span>{{productData.name}}</span></h4>
        <h4>Live Price: <span>{{{productData.price2}}}</span></h4>
        <h4>Finance: <span>{{{finance productData.price2 "UK"}}}</span></h4>
        <h4>Price Label2: <span>{{{productData.pricelabel2}}}</span></h4>
        <h4>saving label &amp; value: <span>{{productData.savinglabel}} {{{productData.savingvalue}}}</span></h4>
        <h4>Was: <span>{{{productData.wasprice}}}</span></h4>
      </div>
    {{/if}}
  <style>
  /* June 13th 2018 */
  .hideCountdown {
    display:none;
  }
  .productDetails {
    font-family: "HelveticaNeueW01-75Bold", Arial, sans-serif;
    font-size: 14px;
    color: #6e14bb;
    margin-top:5px;
    margin-bottom:40px;
  }
  .productDetails span {
    font-family: "HelveticaNeueW01-Thin", Arial, sans-serif;
    font-size: 14px;
    color:#444;
  }
  .productDetails h4 {
    display: inline-block;
    margin: 5px 8px;
  }
  .bannerTextContainer.carCountdown {
    display: table;
  }
  .bannerTextWrapper{
    display: table-cell;
    vertical-align: middle;
  }
  .bannerTextWrapper h2.cdText span {
    margin: 0 1px;
    font-size: 15px !important;
  }
  {{#times 5}}
  .bannerTextContainer[data-messageid="slide_{{inc @index}}"] {
    bottom: {{textBottomPositionL}}px;
    left:{{textLeftPositionL}}px;
  }
  .item.slideIndex{{inc @index}} .linkableRoundel2 {
    position: absolute;
    top: {{EspotImage2_top}}px ;
    {{#if anchorDEspot2Left}}left:{{else}}right:{{/if}} {{EspotImage2_right}}px;
  }
  .slideIndex{{inc @index}} .countdownContainer.HPcd-default.nonCDText h6.cdHeader {
    font-size: {{#if textAFontSize}}{{textAFontSize}}{{else}}20{{/if}}px !important;
    line-height: {{#if textAFontSize}}{{textAFontSize}}{{else}}20{{/if}}px !important;
    color:#{{#if textAColour }}{{csv textAColour 'D'}}{{else}}000000{{/if}} !important;
  }
  .slideIndex{{inc @index}} .countdownContainer.HPcd-default.nonCDText h6.cdFooter {
    font-size: {{#if textBFontSize}}{{textBFontSize}}{{else}}18{{/if}}px !important;
    line-height: {{#if textBFontSize}}{{textBFontSize}}{{else}}18{{/if}}px !important;
    color:#{{#if textBColour}}{{ csv textBColour 'D'}}{{else}}000000{{/if}} !important;
  }

  @media (max-width: 1365px) {
    .bannerTextContainer[data-messageid="slide_{{inc @index}}"] {
      bottom: {{textBottomPositionM}}px;
      left:{{textLeftPositionM}}px;
    }
    .item.slideIndex{{inc @index}} .linkableRoundel2 {
      position: absolute;
      top: {{EspotImage2_top1024}}px ;
      {{#if anchorDEspot2Left}}left:{{else}}right:{{/if}} {{EspotImage2_right1024}}px;
    }
    .slideIndex{{inc @index}} .countdownContainer.HPcd-default.nonCDText h6.cdHeader {
      font-size:{{#if textAFontSize}}{{math textAFontSize "-" 5}}{{else}}14{{/if}}px !important;
      line-height:{{#if textAFontSize}}{{math textAFontSize "-" 5}}{{else}}14{{/if}}px !important;
    }
    .slideIndex{{inc @index}} .countdownContainer.HPcd-default.nonCDText h6.cdFooter {
      font-size:{{#if textBFontSize}}{{math textBFontSize "-" 5}}{{else}}14{{/if}}px !important;
      line-height:{{#if textBFontSize}}{{math textBFontSize "-" 5}}{{else}}14{{/if}}px !important;
    }

  }
  @media (max-width: 1023px) {
    .bannerTextContainer[data-messageid="slide_{{inc @index}}"] {
      bottom: {{textBottomPositionS}}px;
      left:{{textLeftPositionS}}px;
    }
    .item.slideIndex{{inc @index}} .linkableRoundel2 {
      position: absolute;
      top: {{Espot2Image_mobiletop}}px ;
      {{#if anchorMEspot2Left}}left:{{else}}right:{{/if}} {{Espot2Image_mobileright}}px;
    }

  }
  {{/times}}
  @media (max-width: 1365px) {
    .bannerTextWrapper h2.cdText span {
      margin: 0 1px;
      font-size: 12px !important;
    }
  }
@media (max-width: 1023px) {
    {{#if espot2mobile}}
      div.linkableRoundel2 {
        display:block;
      }
      {{else}}
      div.linkableRoundel2 {
        display:none;
      }
    {{/if}}
}

  </style>

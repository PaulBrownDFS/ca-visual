<div class="amp-ca-card-list
{{#iff cards.length '<' 6}}
amp-ca-prod-{{cards.length}}-rows
{{else}}
amp-ca-prod-3-rows
{{/iff}}">
    <div class="amp-ca-card-title">
        {{ header }}
    </div>

    <div class="amp-ca-card-list-wrap">
        {{#each cards}}
            {{dynamicTemplate [@type] ../renderTypes this}}
        {{/each}}
    </div>
</div>